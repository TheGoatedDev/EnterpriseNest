{
  "name": "enterprise-nest",
  "version": "1.0.0",
  "description": "A Enterprise-Level API Framework based on NestJS, built for scaleability, best-practise and robustness. Based on DDD, Onion, Clean and Hexagonal Architecture.",
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "syncpack fix-mismatches || true",
    "------ DEVELOPMENT  ------": "",
    "dev": "tsup --watch",
    "build": "NODE_ENV=production tsup",
    "start": "node dist/index.js",
    "------ GENERATION  ------": "",
    "generate": "turbo gen -c generators/config.ts",
    "------ TESTING  ------": "",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "------ LINTING  ------": "",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "------ FORMATTING  ------": "",
    "format:check": "prettier --check src/*.ts",
    "format": "prettier --write .",
    "------ TYPECHECKING  ------": "",
    "typecheck": "tsc --noEmit",
    "------ DOCS  ------": "",
    "docs": "echo \"Error: no docs specified\" && exit 1",
    "------ MISC  ------": "",
    "clean": "rm -rf dist",
    "------ CI  ------": "",
    "ci": "npm run lint && npm run typecheck && npm run test",
    "------ Docker ------": "",
    "docker:build": "docker build --platform linux/amd64,linux/arm64 -t enterprise-nest ."
  },
  "keywords": [],
  "author": "TheGoatedDev",
  "license": "MIT",
  "devDependencies": {
    "@elsikora/eslint-plugin-sort-decorators": "0.2.8",
    "@faker-js/faker": "8.4.1",
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/testing": "10.3.9",
    "@swc/jest": "0.2.36",
    "@turbo/gen": "2.0.5",
    "@types/compression": "1.7.5",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.14.9",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "6.0.2",
    "@typescript-eslint/eslint-plugin": "7.1.0",
    "@typescript-eslint/parser": "7.1.0",
    "@vercel/style-guide": "6.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.1.0",
