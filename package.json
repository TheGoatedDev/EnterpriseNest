{
    "name": "enterprise-nest",
    "version": "1.0.0",
    "description": "A Enterprise-Level API Framework based on NestJS, built for scaleability, best-practise and robustness. Based on DDD, Onion, Clean and Hexagonal Architecture.",
    "scripts": {
        "------ SETUP/HOOKS ------": "",
        "prepare": "husky || true",
        "preinstall": "syncpack fix-mismatches || true",
        "------ DEVELOPMENT  ------": "",
        "dev": "tsup --watch",
        "build": "NODE_ENV=production tsup",
        "start": "node dist/index.js",
        "------ GENERATION  ------": "",
        "generate": "turbo gen -c generators/config.ts",
        "------ TESTING  ------": "",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "------ LINTING  ------": "",
        "lint": "biome check .",
        "lint:fix": "biome check . --fix",
        "------ TYPECHECKING  ------": "",
        "typecheck": "tsc --noEmit",
        "------ DOCS  ------": "",
        "docs": "echo \"Error: no docs specified\" && exit 1",
        "------ MISC  ------": "",
        "clean": "rm -rf dist",
        "------ CI  ------": "",
        "ci": "npm run lint && npm run typecheck && npm run test",
        "------ Docker ------": "",
        "docker:build": "docker build --platform linux/amd64,linux/arm64 -t enterprise-nest .",
        "docker:run": "docker run -p 3000:3000 enterprise-nest",
        "------ Template ------": "",
        "template:update": "./scripts/template-remote.sh"
    },
    "keywords": [],
    "author": "TheGoatedDev",
    "license": "MIT",
    "devDependencies": {
        "@faker-js/faker": "8.4.1",
        "@golevelup/ts-jest": "0.5.0",
        "@nestjs/jwt": "10.2.0",
        "@nestjs/testing": "10.3.9",
        "@swc/jest": "0.2.36",
        "@types/compression": "1.7.5",
        "@types/express": "4.17.21",
        "@types/jest": "29.5.12",
        "@types/jsonwebtoken": "9.0.6",
        "@types/node": "20.14.9",
        "@types/passport-jwt": "4.0.1",
        "@types/passport-local": "1.0.38",
        "@types/supertest": "6.0.2",
        "@biomejs/biome": "1.8.3",
        "husky": "9.0.11",
        "inquirer": "9.3.1",
        "jest": "29.7.0",
        "lint-staged": "15.2.7",
        "passport-jwt": "4.0.1",
